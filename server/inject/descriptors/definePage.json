{
  "type": "object",
  "properties": {
    "isdefine": {
      "type": "identifier",
      "selector": ".menusection",
      "control": "exists",
      "selectContains": "> Properties",
      "frameSelector": [
        "[name=contentframe]",
        "[name=propertyFrame]"
      ]
    },
    "viewBuildsClick": {
      "type": "action",
      "selector": "a",
      "control": "action.link",
      "selectContains": "Build",
      "frameSelector": "[name=propertyFrame]"
    }
  },
  "connect": [
    {
      "match": [
        "isdefine"
      ],
      "action": "viewBuildsClick"
    }
  ]
}
